{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "NodeNext",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/db/*": [
        "src/db/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/commands/*": [
        "src/commands/*"
      ],
      "@/events/*": [
        "src/events/*"
      ],
      "@/plugins/*": [
        "src/plugins/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/services/*": [
        "src/services/*"
      ],
    }
  }
}
